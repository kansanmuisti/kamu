<!-- ========================================================================= 	-->

<!-- Puhemiehen paatossanat	-->
<!ELEMENT pmpsanat	- - (ot2, (pmpuhe|ptkviitt)) >
<!ATTLIST pmpsanat	ylatun		CDATA	#IMPLIED	
			kohtanro	CDATA	#REQUIRED>
			<!-- Ylatunnisteen teksti -->

<!-- Poytakirjaviittaus -->
<!ELEMENT ptkviitt	- - (%TEKSTI;|ptkviite|viiva)+ >

<!-- ========================================================================= 	-->
<!--    		 ISTUNNON PAATTAMINEN					-->
<!-- ========================================================================= 	-->

<!-- Istunnon paattam; Seuraava istunto, lopetusaika, vakuusteksti	-->
<!ELEMENT paattam	- - (te*, seur?, pmpvuoro?, tyhjanel*, lopetus?, tyhja*, vakuus?)	>
<!ATTLIST paattam 	pnro	CDATA	#IMPLIED
			puhnimi	CDATA	#IMPLIED 
			ylatun	CDATA	#IMPLIED 
			ptkviite	CDATA	#IMPLIED >

			<!-- Attribuutit: --
			-- pnro		Puheenvuoron numero --
			-- puhnimi	Puhujan nimi 
					(kaytetaan ylatunnisteen tyhjentamiseen) --
			-- ylatun	Ylatunnisteen teksti -->

<!-- Istunnon paattaminen; Suullisen kyselytunnin paattaminen. --
  -- Seuraava istunto, lopetusaika, vakuusteksti -->
<!ELEMENT sktpaat	- - (seur?, tyhjanel*, lopetus, tyhja*, vakuus?) 	>
<!ATTLIST sktpaat	pnro	CDATA	#IMPLIED
			puhnimi	CDATA	#IMPLIED 
			ylatun	CDATA	#IMPLIED 
			ptkviite	CDATA	#IMPLIED >
			<!-- Attribuutit: --
			-- pnro		Puheenvuoron numero --
			-- puhnimi	Puhujan nimi 
					(nakyy ylatunnisteessa paperilla) --
			-- ylatun	Ylatunnisteen teksti -->

<!-- Seuraava istunto; "Puhemies: Eduskunnan seuraava..." 	-->
<!ELEMENT seur		- - (puhemies, te+) 	>

<!-- Lopetus; "Taysistunto lopetetaan klo.."		-->
<!ELEMENT lopetus	- - ((%TEKSTI;)+) 	>

<!-- Vakuus; "Poytakirjan vakuudeksi:  -->
<!ELEMENT vakuus	- - (johdanto, allekir?) >

<!-- Johdanto; Kappale johdantotekstia varten	-->
<!ELEMENT johdanto	- - ((%TEKSTI;)+) >

<!-- Ptk:n allekirjoittaja	-->
<!ELEMENT allekir	- - (#PCDATA) >


<!-- Keskenerainen poytakirja	-->
<!ELEMENT kesken	- - (te | tyhja | tyhjanel)*	>


<!-- ========================================================================= 	-->
<!--    		 TEKSTIN MUOTOILU					-->
<!-- ========================================================================= 	-->

<!-- Sivunvaihto; Elementilla voidaan lisata pakotettu sivunvaihto poytakirjaan  -->
<!ELEMENT svaihto	- O EMPTY >		

<!-- Keskiviiva -->
<!ELEMENT viiva		- O EMPTY >


